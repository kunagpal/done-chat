{
  "name": "done-chat",
  "version": "0.1.1",
  "description": "An elementary chat application built with Done.js",
  "homepage": "http://donechat.herokuapp.com",
  "author": {
    "name": "Kunal Nagpal",
    "email": "kunagpal@gmail.com",
    "url": "https://github.com/kunagpal"
  },
  "scripts": {
    "deploy": "git push heroku master",
    "test": "testee src/test.html --browsers firefox --reporter Spec",
    "start": "can-serve --port 3000",
    "develop": "can-serve --develop --port 3000",
    "document": "documentjs",
    "build": "node build"
  },
  "main": "done-chat/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "chat",
    "elementary",
    "done",
    "js"
  ],
  "system": {
    "main": "done-chat/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload"
    ],
    "npmAlgorithm": "flat"
  },
  "dependencies": {
    "can": "^3.3.0",
    "can-connect": "^1.0.20",
    "can-ssr": "^0.11.6",
    "done-autorender": "^0.8.0",
    "done-component": "^0.5.0",
    "done-css": "~2.0.3",
    "foundation": "^4.2.1-1",
    "generator-donejs": "^0.9.6",
    "jquery": "~3.1.1",
    "steal": "^0.16.43"
  },
  "devDependencies": {
    "can-fixture": "^1.0.10",
    "documentjs": "^0.4.2",
    "donejs-cli": "^0.9.5",
    "donejs-deploy": "^0.4.0",
    "funcunit": "~3.1.0",
    "steal-qunit": "^0.1.4",
    "steal-tools": "^0.16.8",
    "testee": "^0.3.0"
  }
}
